{
    "name": "jorzelalves/focus-php",
    "description": "A package to php integration with focusNFe",
    "keywords": ["focusnfse", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Jorzel",
        "email": "jorzelalves@hotmail.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0",
        "ext-json": "*"
    },
